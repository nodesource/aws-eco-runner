{
  "name": "aws-eco-runner",
  "version": "1.0.0",
  "description": "Automate the management of your GitHub Actions runner on AWS to minimize costs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "ncc build -o dist/ src/main.js && rm -rf dist/package.json",
    "format-check": "biome format ./",
    "format": "biome format --write ./",
    "lint": "biome lint ./",
    "lint-fix": "biome lint --apply ./",
    "test": "echo \"Tests TODO\" && exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@aws-sdk/client-ec2": "^3.582.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@vercel/ncc": "^0.38.1"
  }
}
